.plush {
  width: 100%;
  background: url("https://cdn.lljgame.com/wechat_carnival%2Fdetail_bg.png") no-repeat;
  background-size: cover;
  /* height: 100%; */
  /* background: red; */
  overflow: hidden;
}
.plush #popup {
    height: 1250rpx;
    width: 100%;
    position: absolute;
    bottom: -1250rpx;
    right: 0;
    background: -webkit-linear-gradient(top, #c100e4 0%, #6000fd 57%, #2e00c4 100%);
    background: linear-gradient(180deg, #c100e4 0%, #6000fd 57%, #2e00c4 100%);
    -webkit-transform: translateY(0rpx);
            transform: translateY(0rpx);
    -webkit-transition: all .2s ease-in 0ms;
    transition: all .2s ease-in 0ms;
}
.plush .plushList {
    width: 100%;
}
.plush .plushList .robot {
      width: 100%;
      height: 1240rpx;
}
.plush .plushList .preview {
      width: 420rpx;
      height: 240rpx;
      /* background: red; */
      position: absolute;
      left: 166rpx;
      top: 252rpx;
      overflow: hidden;
}
.plush .plushList .preview .previewImg {
        width: 100%;
        height: 100%;
}
.plush .plushList .preview .tit {
        position: absolute;
        bottom: 0;
        right: 0;
        width: 420rpx;
        height: 40rpx;
}
.plush .noes {
    width: 230rpx;
    height: 102rpx;
    background: url("https://cdn.lljgame.com/wechat_carnival%2Fplay_pay.png") no-repeat;
    background-size: cover;
    text-align: center;
    /* font-family:HYQiHei-HES,HYQiHei; */
    font-weight: bold;
    font-size: 36rpx;
}
