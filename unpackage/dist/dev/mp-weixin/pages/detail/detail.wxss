page, page {
  height: 100%;
}
.details {
  width: 100%;
  height: 100%;
  /* overflow: hidden; */
  background: url("https://cdn.lljgame.com/wechat_carnival%2Fdetail_bg.png") no-repeat;
  background-size: cover;
}
.details .productList {
    /* padding:  0 30rpx; */
    background: url("http://m.qpic.cn/psc?/V12lxWdB1s9Mfe/n.zGh5iTxIbbdjg4.d1VA5Hzvcxs13k6XcmFi673BijAipeD0hcNmBzsGaxzxp*cvt0PL9nPy*cthjnlQArtgQ!!/mnull&bo=3AXEBtwFxAYDByI!&rf=photolist&t=5") no-repeat;
    /* background: red; */
    background-size: 100% 100%;
    height: calc(100vh - 600rpx);
    width: 100%;
}
.details .productList .menus {
      height: calc(100vh - 700rpx);
      overflow: scroll;
}
.details .productList .headline {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: vertical;
      -webkit-box-direction: normal;
      -webkit-flex-direction: column;
              flex-direction: column;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      font-size: 32rpx;
      /* background: red; */
      color: #FFFFFF;
      padding-top: 30rpx;
}
.details .productList .proList {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
      -webkit-flex-direction: row;
              flex-direction: row;
      -webkit-justify-content: space-around;
              justify-content: space-around;
      /* align-items: center; */
      /* padding: 0 30rpx; */
      /* padding-left: 30rpx; */
      /* padding-right: 30rpx; */
      /* justify-content: center; */
      -webkit-flex-wrap: wrap;
              flex-wrap: wrap;
      width: 100%;
      /* background: red; */
      /* height: calc(900rpx - 600rpx); */
}
.details .productList .card {
      width: 330rpx;
      height: 150rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: horizontal;
      -webkit-box-direction: normal;
      -webkit-flex-direction: row;
              flex-direction: row;
      font-size: 24rpx;
      background: rgba(0, 0, 0, 0.3);
      border-radius: 10px;
      margin-top: 40rpx;
      color: #FFFFFF;
}
.details .productList .card image {
        width: 150rpx;
        height: 150rpx;
        margin-right: 10rpx;
}
.details .titImg {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    height: 600rpx;
}
.details .titImg image {
      width: 600rpx;
      height: 600rpx;
}

